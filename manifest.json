{
  "manifest_version": 3,
  "name": "Link Extractor Extension",
  "version": "1.0",
  "description": "Extract all visible links from the current web page",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Extract Links"
  },
  "background": {
    "service_worker": "background.js"
  }
}